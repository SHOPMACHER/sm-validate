{
  "name": "@shopmacher/validate",
  "version": "1.0.0",
  "description": "Extensible JavaScript form validation library",
  "main": "lib/validate.js",
  "jsnext:main": "lib/validate.js",
  "module": "lib/validate.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack/dev.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack/prod.js",
    "test": "cross-env BABEL_ENV=test jest",
    "prepublish": "npm run build"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SHOPMACHER/sm-validate.git"
  },
  "keywords": [
    "forms",
    "validation",
    "javascript",
    "ux",
    "errors",
    "messages"
  ],
  "author": {
    "name": "Rico Herwig",
    "email": "r.herwig@shopmacher.de"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SHOPMACHER/sm-validate/issues"
  },
  "homepage": "https://github.com/SHOPMACHER/sm-validate#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-hot-loader": "^1.3.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8"
  }
}
